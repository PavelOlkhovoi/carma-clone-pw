{
  "name": "carma-wuppertal-collab",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libraries/collaboration/carma-wuppertal-collab/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libraries/collaboration/carma-wuppertal-collab"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../../coverage/libraries/collaboration/carma-wuppertal-collab"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "storybook": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libraries/collaboration/carma-wuppertal-collab/wuppertal-collab-submodule",
        "command": "npm run storybook"
      }
    },
    "build-storybook": {
      "executor": "nx:run-commands",
      "outputs": ["libraries/collaboration/carma-wuppertal-collab/wuppertal-collab-submodule/storybook-static"],
      "options": {
        "cwd": "libraries/collaboration/carma-wuppertal-collab/wuppertal-collab-submodule",
        "command": "npm run build-storybook"
      }
    },
    "serve-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libraries/collaboration/carma-wuppertal-collab/wuppertal-collab-submodule",
        "command": "npm run serve-storybook"
      }
    }
  }
}
